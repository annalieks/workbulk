<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Entities group tests">
    <test name="Positive test">
        <groups>
            <run>
                <include name="entity"/>
                <exclude name="negative"/>
            </run>
        </groups>
        <packages>
            <package name="com.univ.workbulk.board"/>
        </packages>
    </test>
    <test name="Negative test">
        <groups>
            <run>
                <include name="negative"/>
                <exclude name="user"/>
            </run>
        </groups>
        <packages>
            <package name="com.univ.workbulk.board"/>
        </packages>
    </test>
</suite>